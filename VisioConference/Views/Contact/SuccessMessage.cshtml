@model  VisioConference.DTO.ContactDTO
@{
    ViewBag.Title = "EmailEnvoyer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<br />
<div class="container body-content">
    <div class="jumbotron text-center">
        <img src="~/Content/img/MsgEnvoyer.png" text-align:center; />
        <br />
        <br />
        @*LOGO & TITRE*@
        <h2 id="h2-accueil"> Merci de nous avoir contacté </h2>

        @*ACCUEIL*@
        <p class="lead">
            Nous prendrons contact avec vous dans les plus brefs délais.
        </p>
        <div class="flex-start">
            <button type="button"
                    class="btn btn-outline-primary" style="border: 2px solid #337ab7; font-size: 18px; ">
                @Html.ActionLink("Terminé", "")
            </button>
        </div>
    </div>
</div>